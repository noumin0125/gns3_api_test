    <link rel="stylesheet" type="text/css" href="<%= WEBSERVER %>/dist/css/loading.css">
</head>
<body class="skin-blue">
    <div class="wrapper">

        <header class="main-header">
          <a href="/" class="logo">gns3 api test</a>
          <nav class="navbar navbar-static-top" role="navigation">
              <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                <span class="sr-only">Toggle navigation</span>
              </a>
          </nav>
      </header>
      <aside class="main-sidebar">
        <section class="sidebar">
          <ul class="sidebar-menu">
            <li class="header">機能一覧</li>
            <li><a href="/resource"><i class="fa fa-circle-o"></i>リソース管理</a></li>
          </ul>
        </section>
      </aside><!-- end sidebar -->
      <div class="content-wrapper">
        <%= yield %>
      </div><!-- end content -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">Version0.1</div>
            <strong>GNS3</strong>
        </footer><!-- end footer -->
    </div><!-- end wrapper -->
    <!-- JS -->

    <!-- jquery -->
    <script src="<%= WEBSERVER %>/plugins/jQuery/jQuery-2.2.3.min.js" type="text/javascript"></script>
    <!-- bootstrap -->
    <script src="<%= WEBSERVER %>/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <!-- adminLTE -->
    <script src="<%= WEBSERVER %>/dist/js/app.min.js" type="text/javascript"></script>
</body>
<script>
var dispLoading = function (msg) {
  if( msg == undefined ){
    msg = "";
  }
  var dispMsg = "<div class='loadingMsg'>" + msg + "</div>";
  if($("#loading").length == 0){
    $("body").append("<div id='loading'>" + dispMsg + "</div>");
  }
}
</script>
</html>
